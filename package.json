{
  "name": "pkl-viewer",
  "displayName": "PKL Viewer",
  "description": "View Python pickle (.pkl) files directly in the VS Code editor.",
  "version": "0.1.0",
  "publisher": "alarm",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/alaramartin/pkl-viewer.git"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "vscode",
    "extension",
    "pkl",
    "pickle",
    "python",
    "viewer"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "engines": {
    "vscode": "^1.102.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pkl-viewer.helloWorld",
        "title": "Hello World"
      }
    ],
    "customEditors": [
      {
        "viewType": "pklViewer.pkl",
        "displayName": "PKL Viewer",
        "selector": [
          {
            "filenamePattern": "*.pkl"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
